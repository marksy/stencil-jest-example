{
  "name": "Example",
  "packageName": "test-example",
  "version": "1.0.0",
  "description": "Example of Stencil 4.7 and Jest 29 failure",
  "license": "MIT",
  "author": {
    "name": "Mark Chambers"
  },
  "files": [
    "dist-stencil",
    "react.d.ts",
    "react.js"
  ],
  "scripts": {
    "start": "stencil build --dev --watch --serve --port 2003",
    "build": "stencil build --ci",
    "test": "stencil test --spec --e2e",
    "test:watch": "yarn test --watch",
    "test:coverage": "stencil test --spec --e2e --coverage --coverageDirectory ./coverage --coverageReporters=cobertura --coverageReporters=html --reporters=jest-junit --reporters=default"
  },
  "main": "dist-stencil/index.cjs.js",
  "module": "dist-stencil/index.js",
  "es2015": "dist-stencil/esm/index.mjs",
  "es2017": "dist-stencil/esm/index.mjs",
  "types": "dist-stencil/types/index.d.ts",
  "collection": "dist-stencil/collection/collection-manifest.json",
  "collection:main": "dist-stencil/collection/index.js",
  "unpkg": "dist-stencil/timely-ui/timely-ui.js",
  "distDirs": {
    "stencil": "dist-stencil"
  },
  "jest-junit": {
    "outputDirectory": "coverage",
    "outputName": "jest-junit.xml"
  },
  "dependencies": {
    "@stencil/core": "^4.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/postcss": "^2.1.0",
    "@stencil/react-output-target": "^0.5.3",
    "@stencil/sass": "^3.0.5",
    "@types/jest": "^29.5.7",
    "@types/react": "^16.14.43",
    "@types/react-dom": "^16.9.19",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.3",
    "css-has-pseudo": "^4.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-junit": "^14.0.1",
    "puppeteer": "^19.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stencil-inline-svg": "^1.1.0",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@3.6.3"
}
